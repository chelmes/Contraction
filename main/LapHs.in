# This is an example input file

# lattice parameters:
Lt = 48
Lx = 20 
Ly = 20 
Lz = 20 

# configuration handling:
start_config = 714
end_config   = 714
delta_config = 4

# eigenvector handling:
number_of_eigen_vec = 120
path_eigenvectors   = /data/LapHs/contraction_Markus/test_data/ev
name_eigenvectors   = eigenvectors
# perambulator handling
path_perambulators  = /data/LapHs/contraction_Markus/test_data/peram/strange
name_perambulators  = perambulator
# output path
output_path = ./
lattice = A40.24

# quarks to be used
[quarks]
quark = s:5:TI:2:EI:6:DI:4:/data/LapHs/contraction_Markus/test_data/peram/strange
quark = u:5:TI:2:EI:6:DI:4:/data/LapHs/contraction_Markus/test_data/peram/up

# operators to be used
[operator_lists]
operator_list = g5.d0.p0

# all correlation functions which will be computed !!MUST BE AT END OF FILE!!
[correlator_lists]
correlator_list = C2+:Q0:Op0:Q0:Op0

